{
  "name": "prometheus",
  "version": "1.0.0",
  "main": "server.js",
  "author": "Wiltomar DUARTE <wiltomar@yahoo.com.br>",
  "license": "MIT",
  "scripts": {
    "build": "yarn cache clean && babel src --extensions \".js,.ts\" --ignore=src/test --out-dir dist -s --presets typescript --source-maps",
    "start": "babel-node dist/server.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --inspect --pretty --transpile-only --ignore-watch node_modules --no-notify src/server.ts",
    "typeorm": "ts-node --require ts-node/register ./node_modules/typeorm/cli.js",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/node": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-typescript": "^7.12.13",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/ini": "^1.3.30",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.25",
    "@types/node-cron": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "babel-preset-typescript": "^7.0.0-alpha.19",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "javascript-obfuscator": "^2.16.0",
    "jest": "^26.6.3",
    "prettier": "2.0.5",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^26.5.0",
    "ts-node-dev": "^1.1.1",
    "tsc-alias": "^1.3.5",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.12.13",
    "@types/dotenv": "^8.2.0",
    "@types/helmet": "^4.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.1.1",
    "ini": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mssql": "^7.1.3",
    "node-cron": "^2.0.3",
    "ts-dotenv": "^0.8.3",
    "typeorm": "^0.2.30"
  },
  "_moduleAliases": {
    "@controllers": "src/controllers",
    "@models": "src/models",
    "@views": "src/views",
    "@config": "src/config",
    "@common": "src/common",
    "@middlewares": "src/middlewares"
  }
}
